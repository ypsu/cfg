# GMP debug functions
define pz
set __gmpz_dump($arg0)
end

define pq
set __gmpz_dump(&$arg0->_mp_num)
echo /\n
set __gmpz_dump(&$arg0->_mp_den)
echo \n
printf "%f\n", ((double (*) (long)) __gmpq_get_d)($arg0)
end

define pf
set __gmpf_dump($arg0)
end
